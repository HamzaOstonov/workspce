		<toolbar id="tb" visible="false">
			<toolbarbutton id="btn_save" label="${labels.save}"
				type="submit" image="/images/save.png" />
			<toolbarbutton id="btn_clear" label="Очистить" />
		</toolbar>

		<window id="sector_wnd" mode="modal" closable="true" width="85%"
