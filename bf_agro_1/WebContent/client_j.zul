									mold="rounded" hflex="3" readonly="true"
									selecteditem="@{clientmain$composer.newcl.accountant.code_country}" />
							</cell>
							<cell align="end">
								<label hflex="1"
									value="${labels.client.code_adr_region}" />
							</cell>
							<cell>
								<textbox
									id="aaccountant_code_adr_region_text" hflex="1" mold="rounded"
									value="@{clientmain$composer.newcl.accountant.code_adr_region}" />
								<rcombobox
									id="aaccountant_code_adr_region" mold="rounded" hflex="3"
									readonly="true"
									selecteditem="@{clientmain$composer.newcl.accountant.code_adr_region}" />
							</cell>
							<cell align="end">
								<label
									value="${labels.client.code_adr_distr}" />
							</cell>
							<cell>
								<textbox
									id="aaccountant_code_adr_distr_text" mold="rounded" hflex="1"
									value="@{clientmain$composer.newcl.accountant.code_adr_distr}" />
								<rcombobox
									id="aaccountant_code_adr_distr" mold="rounded" hflex="3"
									readonly="true"
									selecteditem="@{clientmain$composer.newcl.accountant.code_adr_distr}" />
							</cell>
						</row>
						<row>
							<cell align="end">
								<label
									value="${labels.client.post_address}" />
							</cell>
							<cell colspan="7">
								<textbox id="aaccountant_post_address"
									mold="rounded" hflex="1"
									value="@{clientmain$composer.newcl.accountant.post_address}" />
							</cell>
						</row>
						<row>
							<cell align="end">
								<label
									value="${labels.client.number_tax_registration}" />
							</cell>
							<textbox
								id="aaccountant_number_tax_registration" mold="rounded"
								hflex="1" maxlength="9"
								value="@{clientmain$composer.newcl.accountant.number_tax_registration}" />
							<cell align="end">
								<label
									value="${labels.client.code_tax_org}" />
							</cell>
							<cell colspan="2">
								<textbox
									id="aaccountant_code_tax_org_text" hflex="1" mold="rounded"
									value="@{clientmain$composer.newcl.accountant.code_tax_org}" />
								<rcombobox id="aaccountant_code_tax_org"
									mold="rounded" hflex="3"
									selecteditem="@{clientmain$composer.newcl.accountant.code_tax_org}" />
							</cell>
							<cell align="end">
								<label
									value="${labels.client_p.p_inps}" />
