						<row>
							<cell align="end">
								<label
									value="${labels.client_p.phone_mobile}" />
							</cell>
							<textbox
								value="@{clientmain$composer.newcl.accountant.phone_mobile}" />
							<cell align="end">
								<label
									value="${labels.client_p.phone_home}" />
							</cell>
							<cell colspan="2">
								<textbox
									value="@{clientmain$composer.newcl.accountant.phone_home}" />
							</cell>
							<cell align="end">
								<label value="${labels.client_p.email}" />
							</cell>
							<textbox
								value="@{clientmain$composer.newcl.accountant.email_address}" />
