			position="center,center" visible="false">
			<grid width="100%">
				<columns>
					<column width="10%" />
					<column width="20%" />
					<column width="10%" />
					<column width="10%" />
					<column />
				</columns>
				<rows>
					<row>
						<label value="Название" />
						<textbox id="item" hflex="1" />
					</row>
					<row>
						<label value="Группа секторов" />
						<cell colspan="3" width="100%">
							<rcombobox id="sectorGroups" readonly="true"
								hflex="2" />
						</cell>
					</row>
					<row>
						<cell colspan="3">
							<hlayout>
								<toolbar>
									<toolbarbutton id="find_sector"
										label="Найти" />
									<toolbarbutton id="clear"
										label="Очистить" />
								</toolbar>
							</hlayout>
						</cell>
					</row>
				</rows>
			</grid>
			<listbox id="sectors" rows="15">
				<listhead sizable="true">
					<listheader label="Код" width="20%" />
					<listheader label="Наименование" />
				</listhead>
			</listbox>
			<toolbar>
				<toolbarbutton id="btn_pick" label="Выбрать" />
			</toolbar>
		</window>

		<window id="nibbd_modal" mode="modal" closable="true"
			width="80%" onClose="self.visible = false; event.stopPropagation();"
			title="НИББД" position="center,center" visible="false" />

		<window id="dp_wnd" mode="modal" closable="true"
			contentStyle="overflow: auto" height="700px" width="100%"
			onClose="self.visible = false; event.stopPropagation();"
			title="Создание связки" position="center,center" visible="false">
			<include id="incl_cp" />
		</window>

		<window id="fields_diff_wnd" mode="modal" closable="true"
			width="90%" onClose="self.visible = false; event.stopPropagation();"
