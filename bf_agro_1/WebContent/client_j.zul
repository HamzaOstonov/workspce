			<grid id="diff_grid">
				<columns id="diff_columns" hflex="1" vflex="1">
					<column label="Поле" width="15%" />
					<column label="Локалные данные" />
					<column id="diff_sap_col" label="Данные из SAP" />
					<column id="diff_sap_col_accept"
						label="Принять из SAP" width="10%">
						<checkbox id="check_all_sap" />
					</column>
					<column id="diff_ebp_col" label="Данные из ЕБП" />
					<column id="diff_ebp_col_accept"
						label="Принять из ЕБП" width="10%">
						<checkbox id="check_all_ebp" />
					</column>
				</columns>
				<rows id="diff_rows" />
			</grid>
			<toolbar>
				<toolbarbutton id="btn_apply" label="Сохранить" />
			</toolbar>
		</window>
		<window id="director_search_wnd" mode="modal" closable="true"
			width="70%" onClose="self.visible = false; event.stopPropagation();"
			title="Сравнение данных" position="center,center" visible="false">
			<div>
				<include id="search_incl" src="/customer/SAPSearch.zul" />
			</div>
		</window>
		<window id="clt_dlg_wnd" mode="modal" closable="true"
			width="50%" onClose="self.visible = false; event.stopPropagation();"
